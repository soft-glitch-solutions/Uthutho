{
  "expo": {
    "name": "Uthutho",
    "slug": "uthutho",
    "version": "1.8.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "uthutho",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      },
      "bundleIdentifier": "com.softglitchsolutions.uthutho"
    },
    "android": {
      "package": "com.softglitchsolutions.uthutho",
      "versionCode": 25,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/logo.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-secure-store",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Uthutho to save posts to your photo gallery.",
          "savePhotosPermission": "Allow Uthutho to save posts to your photo gallery.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "45908a0a-a038-4ab8-8a4e-8ff5b7db2db4"
      }
    },
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "backgroundColor": "#000000"
    },
    "owner": "soft-glitch-solution"
  }
}
