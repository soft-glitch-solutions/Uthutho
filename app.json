{
  "expo": {
    "name": "Uthutho",
    "slug": "uthutho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "uthutho",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-ads-admob",
        {
          "androidAppId": "ca-app-pub-1853756758292263~5421703281",
          "iosAppId": "ca-app-pub-1853756758292263~5421703281"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d57defc0-07eb-42d8-9c0c-e0c8df94ce4a"
      }
    },
    "android": {
      "package": "com.softglitchsolutions.uthutho"
    },
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "backgroundColor": "#000000"
    }
  }
}
