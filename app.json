{
  "expo": {
    "name": "Uthutho",
    "slug": "uthutho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "uthutho",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      }
    },
    "web": {
      "favicon": "./assets/images/logo.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-ads-admob",
        {
          "androidAppId": "ca-app-pub-1853756758292263~5421703281",
          "iosAppId": "ca-app-pub-1853756758292263~5421703281"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "45908a0a-a038-4ab8-8a4e-8ff5b7db2db4"
      }
    },
    "android": {
      "package": "com.softglitchsolutions.uthutho"
    },
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "backgroundColor": "#000000"
    },
    "owner": "soft-glitch-solution"
  }
}
